# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/ai_engines_dashboard"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="your-super-secret-jwt-key-here"

# Server
PORT=4000
HOST=0.0.0.0
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW=60000

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# External APIs
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AI Engines Platform
AGENTOPS_API_URL=https://api.agentops.ai
AGENTOPS_API_KEY=your_agentops_api_key
SACRED_CODEX_API_URL=https://api.sacredcodex.ai
SACRED_CODEX_API_KEY=your_sacred_codex_api_key

# Monitoring
HEALTH_CHECK_INTERVAL=30000
METRICS_RETENTION_DAYS=30

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# WebSocket
WS_HEARTBEAT_INTERVAL=30000