{
  "name": "ai-engines-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Engines Platform Dashboard - AgentOps & Sacred Codex",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t ai-engines-dashboard-backend .",
    "docker:run": "docker run -p 4000:4000 ai-engines-dashboard-backend"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "@fastify/websocket": "^10.0.1",
    "@prisma/client": "^6.1.0",
    "argon2": "^0.41.1",
    "fastify": "^4.29.0",
    "socket.io": "^4.8.1",
    "ioredis": "^5.4.1",
    "zod": "^3.24.1",
    "axios": "^1.7.9",
    "cron": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/cron": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.16.0",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "ai-engines",
    "dashboard",
    "agentops",
    "sacred-codex",
    "monitoring",
    "analytics",
    "fastify",
    "typescript"
  ],
  "author": "Mathius Johnson",
  "license": "MIT"
}